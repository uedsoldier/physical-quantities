{
        "K": {
            "to_base": "lambda x: x",
            "from_base": "lambda x: x"
        },
        "°F": {
            "to_base": "lambda x: (x-32)*5/9 + 273.15",
            "from_base": "lambda x: (x-273.15)*9/5 + 32"
        },
        "°C": {
            "to_base": "lambda x: x + 273.15",
            "from_base": "lambda x: x - 273.15"
        }
}